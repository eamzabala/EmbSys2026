void setup() {
  // put your setup code here, to run once:
  pinMode(13, OUTPUT);
}

void loop() {
  digitalWrite(13, HIGH);   // turn the LED on (HIGH is the voltage level)
  delay(100);                       // wait 250 milliseconds.
  digitalWrite(13, LOW);    // turn the LED off by making the voltage LOW
  delay(100);                       // wait 500 milliseconds.
  digitalWrite(13, HIGH);   // turn the LED on (HIGH is the voltage level)
  delay(100);                       // wait 250 milliseconds.
  digitalWrite(13, LOW);    // turn the LED off by making the voltage LOW
  delay(100);                       // wait 500 milliseconds.
  digitalWrite(13, HIGH);   // turn the LED on (HIGH is the voltage level)
  delay(100);                       // wait 250 milliseconds.
  digitalWrite(13, LOW);    // turn the LED off by making the voltage LOW
  delay(1000);                       // wait 500 milliseconds.

  digitalWrite(13, HIGH);   // turn the LED on (HIGH is the voltage level)
  delay(500);                       // wait 250 milliseconds.
  digitalWrite(13, LOW);    // turn the LED off by making the voltage LOW
  delay(500);                       // wait 500 milliseconds.
  digitalWrite(13, HIGH);   
  delay(500);               
  digitalWrite(13, LOW);    
  delay(500);               
  digitalWrite(13, HIGH);   
  delay(500);               
  digitalWrite(13, LOW);    
  delay(1000);                

  digitalWrite(13, HIGH);   
  delay(100);               
  digitalWrite(13, LOW);    
  delay(100);               
  digitalWrite(13, HIGH);   
  delay(100);               
  digitalWrite(13, LOW);    
  delay(100);               
  digitalWrite(13, HIGH);   
  delay(100);                
  digitalWrite(13, LOW);    
  delay(1000);              
}